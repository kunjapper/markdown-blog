<!-- posts.ejs -->
<!DOCTYPE html>
<html>
  <head>
    <title>Blog Posts</title>
    <link rel="stylesheet" href="/styles.css">

  </head>
  <body>
    <h1>Blog Posts</h1>
    <% posts.forEach(post => { %>
      <h2><%= post.title %></h2>
      <p><%= post.content %></p>
      <hr>
    <% }); %>
  </body>
</html>
